div.navigation-container.clearfix
  button.pull-right.btn.btn-primary.icon-large.icon-save( type = 'button', data-bind = 'click : $root.saveGameBuilder' )
  ul.navigation-list
    li
      button.btn.btn-primary.icon-large.icon-home( type = 'button', data-bind = 'click : openDashboard, attr : { disabled : isDashboardMode }' )
    li
      button.btn.btn-primary.icon-large.icon-gamepad( type = 'button', data-bind = 'click : openScreenManager, attr : { disabled : isScreenManagerMode }' )
    li
      button.btn.btn-primary.icon-large.icon-film( type = 'button', data-bind = 'click : openSpriteManager, attr : { disabled : isSpriteManagerMode }' )

